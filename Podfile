# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

def core_pods
  pod 'RxSwift', '~>5.0'
  pod 'RealmSwift', '~> 10'
  pod 'RxBlocking'
end

use_frameworks!

target 'DotaHeroes' do

  # Pods for DotaHeroes
  pod 'SwiftLint'
  pod 'Alamofire', '~> 5.2'
  pod 'SDWebImage', '~> 5.0'
  pod 'RxCocoa', '~>5.0'
  pod 'RxDataSources', '~> 4.0'
  pod 'Lightbox'
  pod 'SnapKit', '~> 5.0.0'
  core_pods
end

target 'DotaHeroesTests' do
  # Pods for Tests
  core_pods
end
